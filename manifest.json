{
    "manifest_version": 3,
    "name": "Texteame",
    "version": "1.0",
    "description": "Automatización de publicación en Texteame.es",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "icons": {
      "512": "icon.png"
    },
    "background": {
      
    },
    "content_scripts": [
      {
        "matches": ["https://texteame.es/node/*", "https://putapasion.com/perfil/nuevo/*" ,"https://www.destacamos.net/*","https://www.mundosexanuncio.com/publicar*" ,"https://www.loquosex.com/publicar-anuncio/*", "https://tuteanuncias.com/listing-form/*" ,"https://vipscort.es/listing-form/*"],
        "js": ["content.js"]
      }
  ],
     "action": {
      "default_popup": "popup.html"
    }
}